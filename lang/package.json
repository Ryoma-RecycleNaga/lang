{
  "name": "lang",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil App Starter",
  "scripts": {
    "build": "stencil build",
    "hydrate": "stencil build -prerender",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "watch:markdown": "nodemon -V -e md -x \"npm run markdown\"",
    "markdown": "echo $1 $2 $3"
  },
  "dependencies": {
    "@stencil/core": "^1.3.3",
    "@stencil/router": "^1.0.1",
    "nodemon": "^2.0.2",
    "showdown": "^1.9.1"
  },
  "license": "MIT"
}
